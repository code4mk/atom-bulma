".text.html":
  'Fontawesome Icon':
    'prefix': 'fa'
    'body': """
    <i class="fa ${1}" aria-hidden="true"></i>${0}
    """
".text.haml":
  'Fontawesome Icon Haml':
    'prefix': 'fa'
    'body': "%i.fa.${1}{\"aria-hidden\" => \"true\"}${0}"
".source.jsx":
  'Fontawesome Icon React':
    'prefix': 'fa'
    'body': """
    <i className="fa ${1}" aria-hidden="true"></i>${0}
    """
